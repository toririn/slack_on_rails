<% if @validate.errors.present? %>
  $('#error_messages').html("<%= @validate.errors.full_messages.join("\n") %>")
  $('#result').html("")
<% elsif @results.empty? %>
  $('#error_messages').html("<%=j br(Constants::RESULTS_EMPTY_MESSAGE) %>")
  $('#result').html("")
<% else %>
  $('#result').html('<%=j render partial: 'layouts/togethers/result', locals: {
    results: @results,
    user_image_list: @user_image_list,
    slack_markdown_processor: @slack_markdown_processor,
  } %>')
  $('#error_messages').html("")
<% end %>
